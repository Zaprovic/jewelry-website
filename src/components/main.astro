---
import SectionOne from '../components/sections/section-one.astro';
import SectionCarousell from './sections/section-carousell.astro';
import SectionTwo from './sections/section-two.astro';
---


<main>
    <SectionOne/>

    <SectionTwo/>

    <SectionCarousell/>
</main>


<style>
    main {
        display: flex;
        flex-direction: column;
        gap: 20rem;
    }

    @media(min-width: 678px){
        main {
            gap: 0;
        }
    }
</style>

